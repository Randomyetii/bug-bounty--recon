# Environment configuration template
# Copy this file to .env and modify values as needed
# DO NOT commit .env file to version control

# Tool Configuration
MASSCAN_RATE=1000
PORTS=1-1000
HTTPX_THREADS=75
NUCLEI_THREADS=50

# Timeout Configuration (in seconds)
DNS_TIMEOUT=10
MASSCAN_TIMEOUT=300
NMAP_TIMEOUT=600
HTTPX_TIMEOUT=10
NUCLEI_TIMEOUT=1800

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=5

# Output Configuration
ENABLE_CSV_REPORT=true
ENABLE_MARKDOWN_REPORT=true
ENABLE_JSON_REPORT=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_TO_FILE=true

# Security Configuration (sensitive - keep secret)
API_KEY=
WEBHOOKS_URL=

# Notification Configuration
SLACK_WEBHOOK=
DISCORD_WEBHOOK=

# Tool Paths (if custom installation)
AMASS_PATH=amass
MASSCAN_PATH=masscan
NMAP_PATH=nmap
HTTPX_PATH=httpx
NUCLEI_PATH=nuclei